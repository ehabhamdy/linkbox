# Copy to .env and fill in values for production
# For local development, the default values in config.py should work with docker-compose

ENVIRONMENT=dev
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-bucket-name
DATABASE_URL=postgresql://linkbox_user:linkbox_password@localhost:5432/linkbox_dev
PRESIGNED_EXPIRY_SECONDS=3600
MAX_UPLOAD_BYTES=10485760
CLOUDFRONT_DOWNLOAD_DOMAIN=yourdistro.cloudfront.net
